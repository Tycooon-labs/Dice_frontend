{
	"headers": [
		{
			"source": "/index.html",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "no-store"
				}
			]
		},
		{
			"source": "/(.*).(ttf|otf|woff2|js|css|png|jpg|jpeg|ico)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	],
	"rewrites": [{ "source": "/(.*)", "destination": "/" }]
}
